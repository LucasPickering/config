[
  {
    "context": "Editor",
    "bindings": {
      "ctrl-left": "editor::MoveToPreviousSubwordStart",
      "ctrl-right": "editor::MoveToNextSubwordEnd",
      "ctrl-shift-left": "editor::SelectToPreviousSubwordStart",
      "ctrl-shift-right": "editor::SelectToNextSubwordEnd",
      "ctrl-backspace": "editor::DeleteToPreviousSubwordStart",
      "ctrl-delete": "editor::DeleteToNextSubwordEnd",
      "cmd-ctrl-up": "editor::MoveLineUp",
      "cmd-ctrl-down": "editor::MoveLineDown",
      "ctrl-f12": "editor::FindAllReferences",
      "cmd-\\": "pane::SplitRight",
      "cmd-o": "outline::Toggle",
      "cmd-shift-o": "workspace::Open",
      "cmd-enter": "editor::NewlineBelow",
      "shift-f12": "editor::GoToTypeDefinition",
      "cmd-f12": "editor::GoToImplementation",
      "cmd-alt-enter": "search::ReplaceAll",
      "ctrl-alt-cmd-left": [
        "workspace::MoveItemToPaneInDirection",
        { "direction": "left" }
      ],
      "ctrl-alt-cmd-right": [
        "workspace::MoveItemToPaneInDirection",
        { "direction": "right" }
      ],
      "f1": "editor::OpenDocs",
      "cmd-alt-d": ["editor::SelectNext", { "replace_newest": true }],
      "ctrl-shift-up": "editor::AddSelectionAbove",
      "ctrl-shift-down": "editor::AddSelectionBelow"
    }
  },
  {
    "context": "Editor && showing_completions",
    "bindings": {
      "enter": "editor::Newline"
    }
  }
]
