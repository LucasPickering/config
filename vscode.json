{
  "editor.detectIndentation": true,
  "editor.dragAndDrop": false,
  "editor.find.seedSearchStringFromSelection": true,
  "editor.fontSize": 12,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.formatOnSaveTimeout": 3000,
  "editor.lineDecorationsWidth": 0, // Undocumented
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "ctrlCmd",
  "editor.overviewRulerBorder": false,
  "editor.renderWhitespace": "all",
  "editor.rulers": [80],
  "editor.snippetSuggestions": "top",
  "editor.tabSize": 4,
  "editor.useTabStops": true,
  "editor.wordWrap": "off",
  "eslint.packageManager": "yarn",
  "explorer.confirmDelete": false,
  "files.exclude": {
    "**/build": true,
    "**/dist": true,
    "**/node_modules": true,
    "*.pyc": true
  },
  "files.insertFinalNewline": true,
  "files.trimTrailingWhitespace": true,
  "gitHistory.hideCommitViewExplorer": true,
  "gitHistory.showEditorTitleMenuBarIcons": false,
  "gitlens.blame.avatars": false,
  "gitlens.blame.format": "${author|8} | ${message|32?} ${agoOrDate|14-}",
  "gitlens.codeLens.authors.enabled": false,
  "gitlens.codeLens.enabled": false,
  "gitlens.currentLine.enabled": false,
  "prettier.eslintIntegration": true,
  "rust.build_on_save": true,
  "rust.clippy_preference": "on",
  "search.location": "panel",
  "window.zoomLevel": 0,
  "workbench.activityBar.visible": false,
  "workbench.colorTheme": "One Monokai",
  "workbench.editor.closeEmptyGroups": false,
  "workbench.editor.enablePreviewFromQuickOpen": false,
  "workbench.iconTheme": "material-icon-theme",
  "workbench.statusBar.visible": true,
  "rust.all_features": true,
  "rust-client.revealOutputChannelOn": "warn",
  "explorer.confirmDragAndDrop": false,
  "editor.acceptSuggestionOnCommitCharacter": false,
  "python.pythonPath": "python3"
}
